{
  "name": "stackedit",
  "private": true,
  "version": "6.6.6",
  "description": "免费, 开源, 功能齐全的 Markdown 编辑器",
  "author": "Benoit Schweblin, 豆萁, doulongfei",
  "license": "Apache-2.0",
  "type": "module",
  "bugs": {
    "url": "https://github.com/doulongfei/stackedit-dou/issues"
  },
  "scripts": {
    "dev": "vite",
    "build": "NODE_OPTIONS=\"--max-old-space-size=4096\" vite build --mode production && npm run build-style",
    "build-style": "vite build --config vite.style.config.js && copyfiles -f --up=1 diststyle/static/fonts/* dist/static/fonts && copyfiles -f --up=1 diststyle/style.css* dist",
    "preview": "vite preview",
    "copystatic": "copyfiles -f --up=1 diststyle/style.css* dist"
  },
  "dependencies": {
    "@mdit/plugin-img-size": "^0.22.3",
    "@vue/test-utils": "^2.3.2",
    "abcjs": "^6.2.0",
    "bezier-easing": "^2.1.0",
    "body-parser": "^2.2.1",
    "diff-match-patch": "^1.0.5",
    "file-saver": "^2.0.5",
    "handlebars": "^4.7.8",
    "indexeddbshim": "^16.1.0",
    "js-yaml": "^4.1.0",
    "katex": "^0.16.21",
    "markdown-it": "^14.1.0",
    "markdown-it-abbr": "^2.0.0",
    "markdown-it-deflist": "^3.0.0",
    "markdown-it-emoji": "^3.0.0",
    "markdown-it-footnote": "^4.0.0",
    "markdown-it-mark": "^4.0.0",
    "markdown-it-pandoc-renderer": "^1.2.0",
    "markdown-it-sub": "^2.0.0",
    "markdown-it-sup": "^2.0.0",
    "mermaid": "^10.9.3",
    "mitt": "^3.0.0",
    "mousetrap": "^1.6.5",
    "normalize-scss": "^8.0.0",
    "prismjs": "^1.9.0",
    "turndown": "^7.1.2",
    "vue": "^3.5.13",
    "vuex": "^4.1.0"
  },
  "devDependencies": {
    "@npmcli/fs": "^3.1.0",
    "@vitejs/plugin-vue": "^6.0.2",
    "@vue/compiler-sfc": "^3.2.47",
    "autoprefixer": "^10.4.14",
    "chalk": "^5.2.0",
    "copyfiles": "^2.4.1",
    "crypto-js": "^4.2.0",
    "css-loader": "^6.7.3",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.18.2",
    "file-loader": "^6.2.0",
    "http-proxy-middleware": "^2.0.6",
    "identity-obj-proxy": "^3.0.0",
    "ignore-loader": "^0.1.2",
    "jest": "^29.5.0",
    "jest-raw-loader": "^1.0.1",
    "jest-serializer-vue": "^3.1.0",
    "postcss-url": "^10.1.3",
    "replace-in-file": "^8.3.0",
    "rollup-plugin-visualizer": "^6.0.5",
    "sass": "^1.62.0",
    "sass-loader": "^13.2.2",
    "shelljs": "^0.10.0",
    "string-replace-loader": "^3.1.0",
    "stylelint": "^15.4.0",
    "stylelint-config-standard": "^32.0.0",
    "stylelint-processor-html": "^1.0.0",
    "terser": "^5.44.1",
    "vite": "^7.2.4",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-stylelint": "^6.0.0",
    "worker-loader": "^3.0.8"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.3.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ]
}
