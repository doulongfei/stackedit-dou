{
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/chatgpt-api/(.*)",
      "destination": "http://lfdou.dpdns.org:8066/$1"
    },
    {
      "source": "/conf",
      "destination": "/api/index.py"
    },
    {
      "source": "/oauth2/githubToken",
      "destination": "/api/index.py"
    },
    {
      "source": "/oauth2/giteeToken",
      "destination": "/api/index.py"
    },
    {
      "source": "/oauth2/giteaToken",
      "destination": "/api/index.py"
    },
    {
      "source": "/oauth2/gitlabToken",
      "destination": "/api/index.py"
    },
    {
      "source": "/giteeClientId",
      "destination": "/api/index.py"
    },
    {
      "source": "/googleDriveAction",
      "destination": "/api/index.py"
    },
    {
      "source": "/pdfExport",
      "destination": "/api/index.py"
    },
    {
      "source": "/pandocExport",
      "destination": "/api/index.py"
    },
    {
      "source": "/oauth2/callback",
      "destination": "/oauth2/callback.html"
    },
    {
      "source": "/landing/index.html",
      "destination": "/landing/index.html"
    },
    {
      "source": "/",
      "destination": "/landing/index.html"
    },
    {
      "source": "/app",
      "destination": "/index.html"
    },
    {
      "source": "/privacy_policy.html",
      "destination": "/landing/privacy_policy.html"
    },
    {
      "source": "/share.html",
      "destination": "/landing/share.html"
    },
    {
      "source": "/gistshare.html",
      "destination": "/landing/gistshare.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
